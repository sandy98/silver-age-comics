<a style="margin-bottom: 10px;" href="#contents" data-path="{{ path }}">
  {{#if isDirectory }}
    <img src="img/Superman_Folder.ico" />
  {{else }}
    {{#if isComic }}
      <img class="img-polaroid" src="page?page=0&at={{ path }}" width="10%" height="10%" />
    {{/if}}
  {{/if }}
  {{ name }}
</a>